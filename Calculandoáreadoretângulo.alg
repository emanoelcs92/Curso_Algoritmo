algoritmo "Calculandoáreadoretângulo"
// Função :
// Autor :
// Data : 12/07/2022
// Seção de Declarações 
var
   A, b, h: real
inicio
escreva("Digite a base (b) do rerângulo? ")
leia(b)
escreva("Digite a altura (h) do retângulo? ")
leia(h)
A <- b*h
escreva("A área do retângulo é:",A,"cm")
fimalgoritmo
